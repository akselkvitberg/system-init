{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [        
        {
            "background": "#ffe9aa",
            "foreground": "#100e23",
            "leading_diamond": "\ue0b6",
            "properties": {
              "root_icon": "ðŸ¤–"
            },
            "style": "diamond",
            "template": " \uf0e7 ",
            "trailing_diamond": "\ue0b4",
            "type": "root"
        },
        {
          "background": "#bd93f9",
          "foreground": "#100e23",
          "leading_diamond": "\ue0b6",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": "\ue0b1",
            "home_icon": "\ue62a",
            "style": "letter",
            "windows_registry_icon": "\uf071"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#95ffa4",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_gone_icon": "\u2262",
            "branch_icon": "\ue0a0",
            "branch_identical_icon": "\u2261",
            "cherry_pick_icon": "\ue29b ",
            "detached_icon": "\uf839 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "rebase_icon": "\uf47f ",
            "tag_icon": "\uf9f8"
          },
          "style": "powerline",
          "template": " {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}\uf044{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }}\uf046{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#8be9fd",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "type": "node"
        }
      ],
      "type": "prompt"
    },
    {
        "alignment": "right",
        "segments": [
          {
            "background": "#ff79c6",
            "foreground": "#FFFFFF",
            "leading_diamond": "\ue0b2",
            "properties": {
              "time_format": "15:04:05"
            },
            "style": "diamond",
            "template": " {{ .CurrentDate | date .Format }} \uf64f",
            "type": "time"
          },
          {
            "background": "#ff79c6",
            "foreground": "#d6deeb",
            "properties": {
              "style": "roundrock",
              "threshold": 0
            },
            "style": "diamond",
            "template": " {{ .FormattedMs }} ",
            "trailing_diamond": "\ue0b4",
            "type": "executiontime"
          }
        ],
        "type": "prompt"
      },
      {
        "type": "newline"
      },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": "\ue285 ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 1
}
